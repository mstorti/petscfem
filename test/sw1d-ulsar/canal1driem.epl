<:                              # starts ePerl block
#__TRANSCRIPT__
require "$ENV{'PETSCFEM_DIR'}/test/eperlini.pl";       # Initializes ePerl 
$case_name = $ENV{'case_name'};                   # case name
print "# case_name $case_name\n";
$case_name = 'canal1driem' unless $case_name;

$nx=400;
$Nx=$nx+1;
$L=20;
$u0=0.5;
$h0=1.;
$alpha = 1.0;
$Courant = 1.;
$h = $L/$Nx;
$gravity = 1.;
$urms=sqrt($gravity*$h0);
#$Dt = $Courant*$h/$urms;
$Tend = 50;
$Dt=0.5;
#$nstep = ceil($Tend/$Dt);
$nstep = 1000;
#$wall_angle = 20;
#$pi = 3.14159;
#__END_TRANSCRIPT__
@vars = qw(nx Nx L u0 h0 nstep case_name Courant alpha Tend Dt gravity);
transcript("", @vars);	# print variables on output and transcript this block
octave_export_vars(">canal1driem.data.m.tmp",@vars);
#system "octave -qH canal1d.m";
:>//                            # ends ePerl block

global_options
ndim 2
use_log_vars 0

# SUPG conf
weak_form 0

# time stepping
nnwt 2
<:pr('nstep'):>
alpha 1
steady 0

initial_state "canal1driem.ini.tmp"

# "rotary saving" 
nsaverot 1
nfile 100000
nrec 1
nsave 1
#save_file_pattern "canal1d%d.out005s3.tmp"
#save_file_pattern "canal1d.rslt.tmp"
save_file_pattern "./STEPS/canal1driem.%d.tmp"
save_file "canal1driem.state.tmp"

tol_newton 0
solver "petsc"
preco_type "lu"

# convergence parameters. Internal GMRES loop. 
rtol 1.0e-6
atol 0
dtol 1e10
maxits 20
Krylov_dim 150
#print_linear_system_and_stop 1
#solve_system 1
#print_internal_loop_conv 1
<:pr('Dt'):>
tau_fac 1


__END_HASH__

# ndim nu ndof
nodes   2  3  2
__INCLUDE__ canal1driem.nod.tmp

__END_NODES__

#===========================================================
table physical_data
<:pr('gravity'):>
friction_law "Chezy"
Ch 110.
shape "rectangular"
#radius 2
width 1.
#wall_angle <:=$wall_angle*$pi/180:>
cfric 0.
ndim 2
__END_HASH__

#===========================================================
elemset streamsw1d 2
geometry cartesian1d
npg 2
_table_include physical_data
__END_HASH__
__INCLUDE__ canal1driem.con.tmp
__END_ELEMSET__

elemset streamsw1d_abso 6
geometry cartesian1d
lagrange_diagonal_factor 1.e-5
#lagrange_residual_factor 1.0
#props normaln[1]
props normal[1]
_table_include physical_data
__END_HASH__
__INCLUDE__ canal1driem.rest.tmp
__END_ELEMSET__

end_elemsets

#if 0
fixa
__INCLUDE__ canal1driem.fixa.tmp
__END_FIXA__
#endif

#if 0
constraint
-1.0 51 1 1.0 1 1
-1.0 51 2 1.0 1 2 
__END_CONSTRAINT__
#endif

# Local Variables: $
# mode: shell-script $
# eval: (setq indent-tabs-mode nil) $
# End: $
