<:                              # starts ePerl block
require "$ENV{'PETSCFEM_DIR'}/test/eperlini.pl";       # Initializes ePerl 
require "$ENV{'PETSCFEM_DIR'}/tools/dx.pl";       # Initializes ePerl 
require 'dataw.pl';
:>//                            # ends ePerl block

# <:pr('case_name'):>
#===========================================================
global_options
ndim 2

save_file "wave_mmv.state.tmp"
nsave 1

save_file_pattern "wave_mmv.state_%d.tmp"
nsaverot 1000000
nrec 1
nfile 100000000

nstep 10000
solver "iisd"
iisd_subpart_auto 2000
preco_type "jacobi"
Krylov_dim 200
nnwt 1
alpha 1.0
weak_form 0
steady
<:pr('Dt'):>
rtol 1e-4
atol 1e-10
tol_newton -1.
iisdmat_print_statistics
print_partitioning_statistics
# print_linear_system_and_stop
# verify_jacobian_with_numerical_one 1
# solve_system 0
# report_consumed_time

hook_list dl_generic_hook my_ale_mmv_hook

my_ale_mmv_hook.filename ./alehook.efn
my_ale_mmv_hook.prefix   ale_mmv_hook

<:pr('fs_relax'):>
cyclic_fs
cyclic_length <:=-$Lx:>
#if 0
# For DX
dx_auto_combine
dx_port 5315
#endif
__END_HASH__

#===========================================================
# ndim nu ndof
nodedata   2  2  2
# It's read but in fact it is over-written after
data wave.nod.tmp
__END_HASH__

#===========================================================
#if 1
elemset elasticity 4
Young_modulus 1.
Poisson_ratio 0.3
#else
elemset mesh_move 4
#endif
name mesh
geometry cartesian2d
npg 4
data wave.con.tmp
dx 
__END_HASH__

end_elemsets

#================================================================
fixa
__INCLUDE__ "wave_mmv.fixa_bot.tmp"
__END_FIXA__

#================================================================
fixa_amplitude dl_generic
ext_filename "./alehook.efn"
function_name "fs_coupling"
__END_HASH__
__INCLUDE__ "wave.mmv_top.tmp"
__END_FIXA__

constraint
__INCLUDE__ "wave.mmv_peri.tmp"
__END_CONSTRAINT__

# Local Variables: $
# mode: shell-script $
# eval: (setq indent-tabs-mode nil) $
# End: $
