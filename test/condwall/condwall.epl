# -*- mode: shell-script -*-
#$Id: condwall.epl,v 1.9 2005/04/01 02:39:38 mstorti Exp $
# case: <:=$ENV{'case'}:>
#<: require './condwall.pl'; :>
##---:---<*>---:---<*>---:---<*>---:---<*>---:---<*>---:---<*>---: 
global_options
ndim 2

nsave 5
nstep 10000
save_file condwall.state.tmp

nsaverot 1
save_file_pattern "STEPS/condwall.state_%d.tmp"
nrec 1
nfile 100000000

solver "iisd"
# solver "petsc"
preco_type "jacobi"
iisd_subpart_auto 2000

proc_weights weights.dat
max_partgraph_vertices 2000
max_partgraph_vertices_proc 2000

#    Iterative parameters
tol_tet 1e-5
tol_newton 1e-10
atol 0
rtol 1e-6
tol 1e3
nnwt 2
lagrange_diagonal_factor 1e-6

Krylov_dim 300
maxits 300
print_internal_loop_conv

Dt 0.05
# steady

#if 0
print_linear_system_and_stop
solver_system 0
#endif

#if !$dx
##<:add_hook('dl_generic_hook mov_wall'):>
##<:add_hook('shell_hook cw_shell_hook'):>
mov_wall.filename ./movwall.efn
mov_wall.prefix mov_wall
use_vector_resistance
use_fastmat2_cache 0
#endif

#if $dx
# Para DX
##<:add_hook('ns_dx_hook my_dx_hook'):>
dx_auto_combine
dx_steps 1
dx_read_state_from_file 1
dx_do_make_command
dx_cache_coords 0
dx_cache_connectivities 0
partitioning_method natural
#endif

<:print_hook():>

__END_HASH__

##---:---<*>---:---<*>---:---<*>---:---<*>---:---<*>---:---<*>---: 
# ndim nu ndof
nodes  2  2  3
#if !$dx
__INCLUDE__ condwall.nod.tmp
#else
__INCLUDE__ condwall.nod-dx.tmp
#endif
_END_NODES__

##---:---<*>---:---<*>---:---<*>---:---<*>---:---<*>---:---<*>---: 
elemset nsi_tet_les_fm2 4 
name fluid
geometry cartesian2d
ndim 2
npg 4
#
# Datos fisicos
#
viscosity 0.001
weak_form 0
LES 0
dx 1
__END_HASH__
__INCLUDE__ condwall.con.tmp
__END_ELEMSET__

##---:---<*>---:---<*>---:---<*>---:---<*>---:---<*>---:---<*>---: 
#if 0
elemset qharmm 4 
geometry cartesian2d
npg 4
conductivity 0.
C 0.001 0.001 0.
Cp 0.
__END_HASH__
__INCLUDE__ condwall.con.tmp
__END_ELEMSET__
#endif

#if !$dx && !$use_peri
##---:---<*>---:---<*>---:---<*>---:---<*>---:---<*>---:---<*>---: 
elemset cond_wall 4
name wall
__END_HASH__
__INCLUDE__ condwall.condwall-con.tmp
__END_ELEMSET__
#endif

end_elemsets

##---:---<*>---:---<*>---:---<*>---:---<*>---:---<*>---:---<*>---: 
#if !$dx
fixa
__INCLUDE__ ./condwall.fixa-in.tmp
__INCLUDE__ ./condwall.fixa-out.tmp
# __INCLUDE__ ./condwall.fixa-wall2.tmp
# __INCLUDE__ ./condwall.fixa-wall1.tmp
__END_FIXA__

##---:---<*>---:---<*>---:---<*>---:---<*>---:---<*>---:---<*>---: 
constraint
__INCLUDE__ ./condwall.peri2.tmp
__INCLUDE__ ./condwall.peri1.tmp
#if $use_peri
__INCLUDE__ ./condwall.wall-peri.tmp
#endif
__END_CONSTRAINT__
#endif
