<: require './data.pl'; :>
#===========================================================
global_options
ndim 2
save_file "ext.state.tmp"
nsave 1
## should stop on signal from cylin.epl
nstep 10000000
save_file_pattern "ext.out_%d.tmp"
nsaverot 1
steady
solver "iisd"
iisd_subpart_auto 2000
print_internal_loop_conv 1
Krylov_dim 200
nnwt 1
Uinf 1.
inv_lay_inc <:= 3*$Ntheta/4+2*$Nx+1 :>

weak_form 0
Dt <:=$Co*$hmin:>
rtol 1e-4
atol 1e-10

iisdmat_print_statistics
print_partitioning_statistics
hook_list dl_generic_hook coupling_inv_hook 

coupling_inv_hook.filename ./inviscid.efn
coupling_inv_hook.prefix   coupling_inv_hook

# Filter
<: $omega=0.9; $xif=0.1; :>
n_coef 3
a_coef <:=1./$omega**2+$xif/$omega+0.5:> <:=-2/$omega**2:> <:=1./$omega**2-$xif/$omega+0.5:> 
b_coef 0.5 0. 0.5
__END_HASH__

#===========================================================
# ndim nu ndof
nodedata   2  2  1
data ext.nod.tmp
__END_HASH__

#===========================================================
elemset qharm 4
geometry cartesian2d
npg 4
data ext.con.tmp
__END_HASH__

end_elemsets

#===========================================================
fixa_amplitude dl_generic
ext_filename "./inviscid.efn"
function_name "coupling"
__END_HASH__
__INCLUDE__ "ext.fixa_ext.tmp"
__END_FIXA__

