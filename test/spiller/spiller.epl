##__INSERT_LICENSE__
## $Id: spiller.epl,v 1.2 2003/03/14 21:32:04 mstorti Exp $ 
<:                              # starts ePerl block
#__TRANSCRIPT__
require "$ENV{'PETSCFEM_DIR'}/test/eperlini.pl";       # Initializes ePerl 
require "$ENV{'PETSCFEM_DIR'}/tools/dx.pl";       # Initializes ePerl 
$case_name = $ENV{'case_name'};
$case_name = 'default' unless defined $case_name;
require './data.pl';
if (!$re_start && -f 'spiller.axis_nodes.rslt.tmp') {
    unlink 'spiller.axis_nodes.rslt.tmp';
}
$layers = 2;
#__END_TRANSCRIPT__
@vars = qw(Rint Rext Rext2 L Ntheta Nr Nx du_ini_pert 
    Next re_start layers Dt);
transcript("", @vars);	# print variables on output and transcript this block
octave_export_vars(">data.m.tmp",@vars);
system "octave -qH spiller.m";
$nnod = count_lines("./spiller.nod.tmp");
:>//                            # ends ePerl block

# <:pr('case_name'):>
#===========================================================
global_options
ndim 2

save_file "spiller.state.tmp"
nsave 10

save_file_pattern "spiller.state_%d.tmp"
<:pr('nsaverot'):>
nrec 10
nfile 100000000

initial_state <:= ($re_start ? "spiller.state.tmp" : "spiller.ini.tmp") :>
nstep 10000
# steady
solver "iisd"
iisd_subpart_auto 2000
preco_type "jacobi"
print_internal_loop_conv 1
Krylov_dim 200
nnwt 3
alpha 0.6
weak_form 0
<:pr('Dt'):>
rtol 1e-4
atol 1e-10
iisdmat_print_statistics
print_partitioning_statistics
# print_linear_system_and_stop
# verify_jacobian_with_numerical_one 1
# solve_system 0

hook_list ns_dx_hook my_dx_hook
viscosity <:=2*$Rint/$Re:>

# Para DX
# hook_list ns_dx_hook my_dx_hook
dx_split_state "1 2 0 1 u 1 1 2 p"
dx_auto_combine
dx_state_all_fields 0
dx_steps 1
__END_HASH__

#===========================================================
# ndim nu ndof
nodedata   2  2  3
data spiller.nod.tmp
__END_HASH__

#===========================================================
elemset nsi_tet_les_fm2 4
name viscous_fluid_elemset
geometry cartesian2d
npg 4
data spiller.con.tmp
dx 
__END_HASH__

end_elemsets

#===========================================================
fixa
__INCLUDE__ "spiller.outlet.tmp"
__INCLUDE__ "spiller.skin.tmp"
__END_FIXA__

#===========================================================
fixa_amplitude dl_generic
ext_filename "./inviscid.efn"
function_name "visc_coupling"
__END_HASH__
__INCLUDE__ "spiller.nod_ext_fix.tmp"
__END_FIXA__

#===========================================================
constraint
__INCLUDE__ "spiller.normal.tmp"
__END_CONSTRAINT__


# Local Variables: $
# mode: shell-script $
# eval: (setq indent-tabs-mode nil) $
# End: $
