:>//                            # ends ePerl block

#default definitions
#===========================================================
global_options
ndim 2

save_file "save.state"
nsave 5

#if 1
solver "petsc"
#else
# solver "iisd"
iisd_subpart 1
#endif
nnwt 1
pc_lu_fill 8.
rtol 1e-4
atol 1e-10
maxits 100
Dt 1
steady
c_volume 0.

<:$nstep = 5;:>//
nstep <:=$nstep:>
displ_factor <:=1./$nstep:>

# print_linear_system_and_stop

__END_HASH__

