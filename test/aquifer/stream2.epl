# -*- mode: shell-script -*-
global_options

ndim 2
nsave 5
nstep 1
initial_state "stream2.ini"

solver "petsc"
preco_type "lu"

# file containing the weight (speed) of processors
proc_weights weights.dat

#    Iterative parameters
tol_tet 1e-5
tol_newton 1e-10
atol 0
rtol 1e-6
dtol 1e3
nnwt 1

Krylov_dim 10
print_internal_loop_conv 1
# print_linear_system_and_stop 1
# verify_jacobian_with_numerical_one 1

# print_nodal_partitioning 1
Dt 1.
# steady <:=$steady:>
alpha 1.
weak_form 0
tau_fac 0.1

__END_HASH__

# ndim nu ndof
nodes  2  3  1
0.  0.0   0.0e-2
1.  0.0  -1.0e-2
2.  0.0  -2.0e-2
3.  0.0  -3.0e-2
4.  0.0  -4.0e-2
5.  0.0  -5.0e-2
6.  0.0  -6.0e-2
7.  0.0  -7.0e-2
8.  0.0  -8.0e-2
9.  0.0  -9.0e-2
10. 0.0  -8.0e-2
_END_NODES__

elemset stream 2
props slope
name malla
geometry cartesian1d
npg 2
friction_law "Chezy"
Ch 1.
shape "rectangular"
width 1.
slope 0.01
__END_HASH__
1 2  -0.01
2 3  -0.01
3 4  -0.01
4 5  -0.01
5 6  -0.01
6 7  -0.01
7 8  -0.01
8 9  -0.01
9 10  -0.01
10 11  -0.01
__END_ELEMSET__

end_elemsets

fixa
1 1 1.
11 1 1.
__END_FIXA__
