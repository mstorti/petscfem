<:require 'eperlini.pl':>// # Initializes ePerl 

# Synchronize the time step with external frequency
<:
$N =16;      # number of time steps per period
$omega=2.*$PI*10.; # pulsation
$T=2*$PI/$omega;   # period
$Dt = $T/$N;      # we put $N time steps per period
:>//

global_options

alpha .5
nsave 100000000
save_file oscplate.sal
#nstep <:=round($N*1.25):>
nstep 1

nsaverot   1
nrec       1000000
nfile      1

print_some_file <:heredoc(<<EOF,"tmp_file_some.tmp");
11
EOF
_:>
save_file_some oscsome3.sal
nsome 1

#    Iterative parameters
nnwt 3
maxits 20
Krylov_dim 20

C_smag 0.
Dt <:=$Dt:>
initial_state oscplate3.ini
__END_HASH__

# ndim nu ndof
nodes  2  2  3
