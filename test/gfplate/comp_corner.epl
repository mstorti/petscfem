# -*- mode: shell-script -*-
# case: <:=$ENV{'case'}:>
## Derived quantities:
## ==================
global_options

# dx <:$dx=1, $ref='';:>

nstep 1000
alpha 1.

#if 0
solver "iisd"
preco_type "jacobi"
iisd_subpart_auto 4000
maxits 100
#else
solver "petsc"
preco_type "none"
maxits 1
#endif

# file containing the weight (speed) of processors
proc_weights weights.dat

#    Iterative parameters
tol_tet 1e-5
tol_newton 1e-10
atol 0
rtol 1e-7
dtol 1e3
nnwt 3

print_internal_loop_conv
# print_linear_system_and_stop
# solve_system 0
Dt 0.4
shocap 0.
weak_form 0
temporal_stability_factor 0

ndim 2
visco 0.1
cond 0.01
steady 0
linear_abso 1

lagrange_diagonal_factor 1e-5
# lagrange_residual_factor 1.0

#if $dx
# Para DX
hook_list advdif_dx_hook my_dx_hook
dx_auto_combine
dx_steps 1
dx_read_state_from_file 1
# dx_do_make_command
dx_cache_coords 0
dx_cache_connectivities 0
dx_port 5320
partitioning_method natural
#endif

__END_HASH__

##================================================================
# ndim nu ndof
nodedata 2 2 4
data ./comp_corner_Ma_5_low_Rey.nod.tmp
__END_HASH__

##================================================================
elemset gasflow 4
geometry cartesian2d
dx 1
npg 4 
data ./comp_corner_Ma_5_low_Rey.con.tmp
__END_HASH__
