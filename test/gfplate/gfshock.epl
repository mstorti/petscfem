# -*- mode: shell-script -*-
# case: <:=$ENV{'case'}:>
# <: require './dshock.pl'; :>
## Derived quantities:
## ==================
global_options

# dx <:=$dx:>

nstep 1000
alpha 1.

initial_state "gfshock.ini.tmp"
## initial_state "gfshock.state.tmp"
nsave 1
save_file "gfshock.state.tmp"

print_some_file "gfshock.some-nodes.tmp"
save_file_some "gfshock.some-rslt.tmp"
nsome 1

save_file_pattern "gfshock.state_%d.tmp"
nsaverot 20
nrec 1
nfile 100000000

#if 0
solver "iisd"
preco_type "jacobi"
iisd_subpart_auto 4000
#else
solver "petsc"
preco_type "lu"
#endif

maxits 100

# file containing the weight (speed) of processors
proc_weights weights.dat

#    Iterative parameters
tol_tet 1e-5
tol_newton 1e-10
atol 0
rtol 1e-7
dtol 1e3
<:pr('Dt'):>
nnwt 3

print_internal_loop_conv
# print_linear_system_and_stop
# solve_system 0
shocap 1
shocap_aniso 1
weak_form 0
temporal_stability_factor 0

ndim 2
Rgas <:=$Rgas:>
visco 1e-8
cond 1e-8
steady 0
gamma <:=$ga:>

# lagrange_diagonal_factor 1e-5

#if $dx
# Para DX
hook_list advdif_dx_hook my_dx_hook
dx_auto_combine
dx_steps 1
dx_read_state_from_file 1
# dx_do_make_command
dx_cache_coords 0
dx_cache_connectivities 0
partitioning_method natural
#endif

__END_HASH__

##================================================================
# ndim nu ndof
nodedata 2 2 4
data ./gfshock.nod.tmp
__END_HASH__

##================================================================
elemset gasflow 4
geometry cartesian2d
dx 1
npg 4
data ./gfshock.con.tmp
__END_HASH__

end_elemsets

#if !$dx
fixa_amplitude ramp
start_value <:=$pout0:>
end_value <:=$pin0:>
start_time 0.0
end_time <:=$tramp:>
__END_HASH__
1 4 1.0
__END_FIXA__

fixa
1 1 <:=$rhoin0:>
# 1 3 0.0
<:=$Nx+1:> 4 <:=$pout0:>
__END_FIXA__

constraint
__INCLUDE__ gfshock.peri.tmp
__INCLUDE__ gfshock.v-peri.tmp
__INCLUDE__ gfshock.slip.tmp
__END_CONSTRAINT__

#endif
